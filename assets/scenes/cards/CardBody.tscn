script/source = "extends KinematicBody

var fx
var explosion
var ice_explosion
var trail
var sfx_hit
var sfx_crit

func _ready():
	fx = $Fx
	explosion = $Fx/Explosion
	ice_explosion = $Fx/IceExplosion
	trail = $Fx/Trail
	sfx_hit = $Fx/SfxHit
	sfx_crit = $Fx/SfxCrit

func explode(ice, card):
	if ice:
		sfx_crit.play()
	if card:
		sfx_hit.play()
	ice_explosion.emitting = ice
	explosion.emitting = true
	trail.emitting = false
	
	# Abandon ship (the card might be freed soon)
	var transform = fx.global_transform
	remove_child(fx)
	$\"/root/Arena\".add_child(fx)
	fx.global_transform = transform

func start_trail():
	trail.emitting = true

func type_card():
	pass
"

